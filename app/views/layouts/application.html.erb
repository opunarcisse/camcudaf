<!DOCTYPE html>
<html>
<head>
  <title>Registration</title>
  <%= stylesheet_link_tag    'custom', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <div id="container" class="navbar">
      
      <div id="header" class="navbar-inner">
      <div  class="authen" class="container">
      <nav>
        <ul class="nav pull-right">
          <li><%= link_to "Home",   :root %> </li>
	  <% if logged_in? %>  <% user = member_logged_in? %>
          <li><%= link_to "Welcome : #{user.last_name} " + " #{user.first_name}", user_path("#{logged_in?}") %> </li>
          <li><%= link_to "Sign Out", logout_path %>   </li>
          <% else %>
          <li><%= link_to "Sign in" , login_path unless controller_name == "sessions" %></li>
          <li><%= link_to "Register" , new_user_path%> </li> 
          <%end%>  
          <li><%= link_to "reports", doky_path %> </li>

 	   <li> <a href="http://www.camcudaf.com" target="_blank">camcudaf.com</a> </li>
        </ul>
      </nav>

      </div>
 
          </div>

      <div id="content">
      
      <%= yield %>
      
      </div>

      <div class="navbar"  id="footer">
  
      <p>  &copy 2014 Soft Solutions Ltd | Ruby on Rails Framework </p>
          </div>
    </div>

</body>
</html>
